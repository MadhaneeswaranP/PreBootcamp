// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 
  var str=userInput[0];
  var len=0;
  for (var i = 0; str[i]; i++) 
        if (str[i] != ' ')
           len++;
  console.log(len);

  //end-here
});
