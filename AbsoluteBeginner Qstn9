// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 
  let a=userInput[0]*1;
  let b=userInput[0]*2;
  let c=userInput[0]*3;
  
  console.log(a+" "+b+" "+c);

  //end-here
});
